<h1>Artists</h1>
<h3>Artist Name: <%= @artist.name %></h3>
<h3>Music Genre</h3><%= @artist.genre %>
<br/>
<h2>Songs</h2>
<% @artist.songs.each do |song|%>
  <%= render "songs/song", { song: song } %>
<% end %>
<br />
<%= link_to "Add Song", new_artist_song_path(@artist), class: "ui button green" %>
<br/>
<br/>
<%= link_to "View Artist", board_artists_path(@board) %>
<%= link_to "Edit Artist", edit_board_artist_path([@board, @artist]) %>
<%= link_to "Remove Artist", [@board, @artist], method: :delete %>
<%= link_to "Back", board_artists_path(@board) %>




